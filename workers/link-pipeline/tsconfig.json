{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "types": [
      "@cloudflare/workers-types"
    ],
    "moduleResolution": "bundler",
    "target": "ES2022",
    "module": "ESNext",
    "lib": [
      "ES2022"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist",
    ".wrangler"
  ]
}
